    "        import numpy as np\n",
    "        import pandas as pd\n",
    "        import plotly.express as px\n",
    "        \n",
    "        # Generating the student grades dataset\n",
    "        np.random.seed(15)\n",
    "        data_grades = {\n",
    "            'Grade': np.random.choice(['A', 'B', 'C', 'D', 'F'], 200),\n",
