    "        \n",
    "        # Creating a violin plot\n",
    "        fig_grades = px.violin(df_grades, y='Score', x='Grade', box=True, points=\"all\",\n",
    "                               title=\"Distribution of Scores Across Grade Categories\")\n",
    "        fig_grades.update_layout(xaxis_title=\"Grade\", yaxis_title=\"Score\")\n",
    "        fig_grades.show()\n",
